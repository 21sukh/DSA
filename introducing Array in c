#include <stdio.h>

int main() {
    // Create an integer array with 5 elements
    int numbers[5] = {2, 4, 6, 8, 10};

    // Print out the third element of the array
    printf("The third element of the array is %d\n", numbers[2]);

    // Change the value of the first element of the array
    numbers[0] = 1;

    // Print out all the elements of the array
    for(int i = 0; i < 5; i++) {
        printf("%d ", numbers[i]);
    }
    printf("\n");

    return 0;
}
